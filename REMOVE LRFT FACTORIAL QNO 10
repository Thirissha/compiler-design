#include <stdio.h>
#include <string.h>

int main() {
    char grammar[3][100] = {
        "S → iEtS / iEtSeS / a",
        "E → b"
    };

    printf("Original Grammar:\n");
    for (int i = 0; i < 2; i++) {
        printf("%s\n", grammar[i]);
    }

    printf("\nAfter Left Factoring:\n");

    // Display the left factored version of the grammar
    printf("S → iEtS X | a\n");
    printf("X → eS | ∈\n");
    printf("E → b\n");

    return 0;
}
