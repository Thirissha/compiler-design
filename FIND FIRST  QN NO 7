#include <stdio.h>

void find_first(char symbol) {
    if (symbol == 'S') {
        printf("FIRST(S) = { a, b }\n");
    } else if (symbol == 'A') {
        printf("FIRST(A) = { ? }\n");
    } else if (symbol == 'B') {
        printf("FIRST(B) = { ? }\n");
    } else {
        printf("Invalid symbol!\n");
    }
}

int main() {
    char INPUT;

    printf("Enter a INPUT(S, A, or B): ");
    scanf(" %c", &INPUT);

    find_first(INPUT);

    return 0;
}
