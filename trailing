#include <stdio.h>
#include <string.h>

void printTrailingSet(char nonTerminal[], char *trailingSet[], int size) {
    printf("TRAILING(%s) = { ", nonTerminal);
    for (int i = 0; i < size; i++) {
        printf("%s", trailingSet[i]);
        if (i < size - 1) {
            printf(", ");
        }
    }
    printf(" }\n");
}

int main() {
    char *trailingE[] = { ")", "id", "+" };
    char *trailingT[] = { ")", "id", "*", "+" };
    char *trailingF[] = { ")", "id", "*", "+", "$" };

    printf("TRAILING sets for the given grammar:\n");

    printTrailingSet("E", trailingE, 3);
    printTrailingSet("T", trailingT, 4);
    printTrailingSet("F", trailingF, 5);

    return 0;
}
